{
    "name": "abhilashpujari/php-restservice",
    "description" : "Simple PHP Wrapper class for making REST API calls",
    "type": "library",
    "keywords": ["http", "rest", "php", "curl", "client", "HTTP client"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abhilash Pujari",
            "email": "abhilashpujari11@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "RestService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RestService\\Tests\\": "tests/"
        }
    },
    "extra": {}
}